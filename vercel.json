{
  "buildCommand": "npm run build",
  "outputDirectory": ".next",
  "installCommand": "npm install",
  "devCommand": "npm run dev",
  "framework": "nextjs",
  "functions": {
    "app/api/**/*.js": {
      "memory": 1024,
      "maxDuration": 10
    }
  },
  "env": {
    "NEXT_PUBLIC_APP_NAME": "RWA.defi",
    "NEXT_PUBLIC_APP_DESCRIPTION": "Real World Assets DeFi Platform",
    "NEXT_PUBLIC_DEBUG": "false"
  }
}